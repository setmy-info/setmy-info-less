{
    "name": "setmy-info-less",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "node ./src/main/js/server.js",
        "build": "npm run css && npm run css-min && npm run copy-html",
        "css": "lessc main.less dist/main.css",
        "css-min": "lessc main.less dist/main.min.css --clean-css",
        "watch": "less-watch-compiler . dist main.less",
        "copy-html": "cp ./src/main/html/index.html dist/",
        "e2e": "npx playwright test src/test/js --config=playwright.config.js",
        "test": "jest"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@playwright/test": "^1.48.1",
        "express": "^4.21.1",
        "jest": "^29.7.0",
        "less": "^4.2.0",
        "less-plugin-clean-css": "^1.6.0",
        "playwright": "^1.48.1"
    }
}
